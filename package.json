{
  "name": "wevoteusa",
  "version": "1.0.0",
  "description": "We Vote front end application",
  "private": true,
  "main": "server.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "dynamic-import-node-babel-7",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "babel-plugin-styled-components",
        {
          "displayName": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-do-expressions",
      "@babel/plugin-proposal-function-bind",
      "@babel/plugin-transform-runtime",
      [
        "babel-plugin-import",
        {
          "libraryName": "@material-ui/core",
          "libraryDirectory": "esm",
          "camel2DashComponentName": false
        },
        "core"
      ],
      [
        "babel-plugin-import",
        {
          "libraryName": "@material-ui/icons",
          "libraryDirectory": "esm",
          "camel2DashComponentName": false
        },
        "icons"
      ]
    ]
  },
  "react-svg-icons": {
    "assetPath": "src/img/global/svg-icons"
  },
  "scripts": {
    "lint": "eslint --format stylish --ext .jsx --ext .js src/js",
    "test": "npm run lint && npx stylelint --config ./.stylelintrc \"./src/sass/**/*\"",
    "start": "webpack-dev-server",
    "start-https-old": "PROTOCOL=HTTPS webpack-dev-server",
    "start-https": "PROTOCOL=HTTPS webpack serve ",
    "start-https-singleBundle": "BUNDLE=SINGLE PROTOCOL=HTTPS webpack serve",
    "start-prod-local": "node ./server-prod-local.js",
    "start-profile": "node --inspect-brk ./node_modules/.bin/gulp",
    "prod": "NODE_ENV=production webpack -p",
    "build": "webpack -p",
    "dev-singleBundle": "BUNDLE=SINGLE NODE_ENV=development webpack -p",
    "prod-singleBundle": "BUNDLE=SINGLE NODE_ENV=production webpack -p",
    "build-dev": "NODE_ENV=development webpack",
    "deps": "npm run deps:missing && npm run deps:extra",
    "deps:missing": "dependency-check package.json",
    "deps:extra": "dependency-check package.json --extra --no-dev --ignore",
    "build:doc": "doctoc --github --title \"## Contents\" ./",
    "autoTest": " mocha --require @babel/register --require tests/component/index.js \"./tests/component/tests.js\" && npm run lint",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@wdio/concise-reporter": "^6.8.1",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "d3-geo": "^2.0.1",
    "d3-selection": "^2.0.0",
    "d3-zoom": "^2.0.0",
    "es6-promise": "^4.2.8",
    "flux": "^3.1.3",
    "font-awesome": "^4.7.0",
    "iframe-resizer": "^4.2.11",
    "jquery": "^3.5.1",
    "keymirror": "^0.1.1",
    "lodash-es": "^4.17.15",
    "lodash.assign": "^4.2.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "object-assign": "^4.1.1",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-delay-render": "^0.1.2",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-iframe-resizer-super": "^0.2.2",
    "react-imported-component": "^6.3.13",
    "react-loadable": "^5.5.0",
    "react-mixin": "^5.0.0",
    "react-phone-number-input": "^3.1.8",
    "react-player": "^2.7.0",
    "react-pure-render": "^1.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-share": "^4.3.1",
    "react-simple-maps": "^2.3.0",
    "react-slick": "^0.27.13",
    "react-stripe-elements": "^6.1.2",
    "react-svg": "^11.1.4",
    "react-text-truncate": "^0.16.0",
    "react-textarea-autosize": "^8.3.0",
    "react-toastify": "^6.2.0",
    "react-toggle": "^4.1.1",
    "react-transition-group": "^4.4.1",
    "react-usa-map": "^1.5.0",
    "styled-components": "^5.2.1",
    "superagent": "^6.1.0",
    "unused-webpack-plugin": "^2.4.0"
  },
  "@comment dependencies": [
    "December 2020:",
    "   Removed 'wdio' '^4.0.0' since it has compile errors relating to the 'fibers' package on Big Sur -- this might be needed for selenium tests",
    "   Replaced 'gulp lint-css'",
    "   Removed 'gulp': '^4.0.2', 'gulp-autoprefixer': '^6.1.0', 'gulp-class-prefix': '^1.0.1', 'gulp-cli': '^2.2.0',",
    "       'gulp-livereload': '^4.0.2', 'gulp-sass': '^4.0.2', 'gulp-sourcemaps': '^2.6.5', 'gulp-stylelint': '^8.0.0',",
    "       'gulp-uglify': '^2.1.2', 'watchify': '^3.11.1',\n"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-do-expressions": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-function-bind": "^7.12.1",
    "@babel/plugin-proposal-function-sent": "^7.12.1",
    "@babel/plugin-proposal-json-strings": "^7.12.1",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.1",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@babel/types": "^7.12.10",
    "@wdio/cli": "^6.10.5",
    "@wdio/local-runner": "^6.10.5",
    "@wdio/mocha-framework": "^6.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^1.12.0",
    "babelify": "^10.0.0",
    "blue-tape": "^1.0.0",
    "browser-sync": "^2.26.13",
    "browserify": "^17.0.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.0",
    "css-loader": "^5.0.1",
    "del": "^6.0.0",
    "dependency-check": "^4.1.0",
    "doctoc": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-wdio": "^6.6.0",
    "express": "^4.17.1",

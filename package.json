    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^4.0.0-beta.0",
    "webpack-split-chunks": "^0.2.1",
    "workbox-webpack-plugin": "^6.0.2",
    "yargs": "^16.2.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.2.0",
    "snyk": "^1.381.1"
  },
  "repository": "https://github.com/wevote/webapp.git",
  "pre-commit": [
    "test",
    "lint"
  ],
  "snyk": true
}
